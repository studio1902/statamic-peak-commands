{
    "name": "studio1902/statamic-peak-commands",
    "autoload": {
        "psr-4": {
            "Studio1902\\PeakCommands\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Studio1902\\PeakCommands\\Tests\\": "tests"
        }
    },
    "extra": {
        "statamic": {
            "name": "Statamic Peak Commands",
            "description": "Statamic Peak Commands addon"
        },
        "laravel": {
            "providers": [
                "Studio1902\\PeakCommands\\ServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.2",
        "statamic/cms": "^5.0"
    },
    "require-dev": {
        "spatie/laravel-ray": "^1.39",
        "orchestra/testbench": "^9.0",
        "laravel/pint": "^1.22"
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    }
}
